进程间通信仍使用原来的atusbd_msg_head报文格式，增加一个新的消息类型5，最后的消息部分使用如下的json

{
	"desktops":[
		{
			"name":"desktop1",
			"ip":"192.168.2.222",
			"port":"6666",
			"gwip":"192.168.2.1",
			"gwport":"4232",
			"username":"user",
			"pwd":"password",
			"usbrule":{
				"listtype":"0",
				"@listtype":"0=黑名单;1=白名单",
				"rulelist":[
					{
						"type":"0",
						"@type":"0=usb厂商规则",
						"vendor":"1455",
						"@vendor":"十进制数(长度2字节)"
						"product":"96",
						"@product":"十进制数(长度2字节)"
					},
					{
						"type":"1",
						"@type":"1=usb接口规则",
						"ifcls":"3",
						"@ifcls":"接口类,十进制数(长度1字节）"
						"ifsubcls":"1",
						"@ifsubcls":"接口子类,十进制数(长度1字节）",
						"ifpro":"1",
						"@ifpro":"接口协议,十进制数(长度1字节）"
					},
					{
						"type":"2",
						"@type":"2=usb设备类",
						"devcls":"9",
						"@devcls":"设备类,十进制数(长度1字节）",
						"devsubcls":"1",
						"@devsubcls":"设备子类,十进制数(长度1字节）",
						"devpro":"1",
						"@devpro":"设备协议,十进制数(长度1字节）"
					}
						
				],
				"exceptionlist":[
					{
						"type":"0",
						"vendor":"1455",
						"product":"96",
						"enable":"1",
						"@enable":"1=允许重定向,0=禁止重定向"
					},
					{
						"type":"1",
						"ifcls":"3",
						"ifsubcls":"1",
						"ifpro":"1",
						"enable":"0",
					},
					{
						"type":"2",
						"devcls":"9",
						"devsubcls":"1",
						"devpro":"1",
						"enable":"1"
					}
				],
				"@exceptionlist":"exceptionlist优先级高于黑白名单，并且是否允许重定向可以针对单个的usb规则进行设置"
			}
		}
   	 ]
}
